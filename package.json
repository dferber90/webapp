    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.9",
    "webpack-dev-middleware": "1.4.0",
    "webpack-dev-server": "1.14.0"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "6.1.18",
    "babel-polyfill": "6.2.0",
