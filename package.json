{
  "name": "webapp",
  "private": true,
  "scripts": {
    "postinstall": "mkdir -p data/db",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:prod": "rimraf build/*; node bundling/build-prod.js",
    "build:dev": "rimraf build/*; node bundling/build-dev.js",
    "build:server": "webpack -w --config bundling/webpack.config.dev.server.js",
    "start:concurrent": "rimraf build/*; concurrent \"node bundling/start-dev.js\" \"node bundling/start-api.js\"",
    "start": "rimraf build/*; node bundling/start-dev.js & node bundling/start-api.js & wait",
    "start:main": "rimraf build/main/*; node bundling/start-dev.js",
    "start:api": "rimraf build/api/*; node bundling/start-api.js & wait",
    "mongo": "mongod --dbpath data/db"
  },
  "devDependencies": {
    "babel-core": "6.2.1",
