{
  "name": "webapp",
  "private": true,
  "scripts": {
    "postinstall": "mkdir -p data/db; mkdir -p data/rethinkdb",
    "test": "npm run lint",
    "build": "rimraf build/*; export NODE_ENV=production; npm run build:api; npm run build:client; npm run build:server; cp package.json build/main/; cp package.json build/api/;",
    "build:client": "webpack --config bundling/webpack.config.prod.client.js",
    "build:server": "webpack --config bundling/webpack.config.prod.server.js",
    "build:api": "webpack --config bundling/webpack.config.prod.server-api.js",
    "start:concurrent": "rimraf build/*; concurrent \"node bundling/start-dev.js\" \"node bundling/scripts/start-api.js\"",
    "start": "rimraf build/*; export NODE_ENV=development; node bundling/start-dev.js & node bundling/scripts/start-api.js & wait",
    "start:main": "rimraf build/main/*; export NODE_ENV=development; node bundling/scripts/start-dev.js",
    "start:api": "rimraf build/api/*; export NODE_ENV=development; node bundling/scripts/start-api.js & wait",
    "mongo": "mongod --dbpath data/db",
    "rethinkdb": "rethinkdb -d data/rethinkdb --driver-port 28015 --http-port 3002",
    "lint": "eslint .",
    "plop": "plop"
  },
  "devDependencies": {
    "babel-core": "6.3.26",
    "babel-eslint": "4.1.5",
    "babel-loader": "6.2.1",
    "babel-plugin-react-transform": "2.0.0",
    "babel-plugin-transform-object-rest-spread": "6.1.18",
    "babel-plugin-transform-react-display-name": "6.3.13",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.1.18",
    "babel-preset-stage-3": "6.3.13",
    "concurrently": "1.0.0",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "3.0.2",
    "eslint-plugin-react": "3.14.0",
    "exists-sync": "0.0.3",
    "fake-file-loader": "1.0.0",
    "fake-style-loader": "1.0.0",
    "fake-url-loader": "1.0.0",
    "file-loader": "0.8.5",
    "http-proxy": "1.12.0",
    "keypress": "0.2.1",
    "less": "2.5.3",
    "less-loader": "2.2.2",
    "nodemon": "1.8.1",
    "plop": "1.4.1",
    "react-transform-catch-errors": "1.0.1",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.2.0",
    "redux-devtools": "3.0.0",
    "rimraf": "2.5.1",
